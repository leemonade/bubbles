{
  "name": "@bubbles-ui/leemons",
  "version": "1.2.251",
  "description": "The Bubbles Design System is Leemonade's open-source design system for products and experiences.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/leemonade/bubbles.git",
    "directory": "packages/leemons"
  },
  "bugs": "https://github.com/leemonade/bubbles/issues",
  "files": [
    "lib/**/*",
    "es/**/*",
    "umd/**/*",
    "types/**/*"
  ],
  "keywords": [
    "react",
    "bubbles",
    "bubbles-leemons",
    "leemons",
    "bubbles-design-system",
    "leemons"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build-publish": "run-s build version:patch publish",
    "version:patch": "npm version patch",
    "start": "yarn storybook",
    "build": "run-s -cs build:clean build:lib build:fix types",
    "build-all": "run-s -cs build",
    "build-all:no-types": "run-s build:no-types build-storybook",
    "build:no-types": "run-s -cs build:clean build:lib build:fix",
    "build:clean": "rimraf es lib umd esm cjs storybook-static build",
    "build:lib": "node scripts/build.js",
    "build:fix": "rimraf es/**/*.mdx lib/**/*.mdx",
    "build-storybook": "run-s -cs build:storybook:clean build:storybook",
    "build:storybook:clean": "rimraf ../../docs-build/leemons",
    "build:storybook": "cross-env NODE_ENV=production build-storybook -o ../../docs-build/leemons",
    "storybook": "run-s storybook:clean storybook:start",
    "storybook:clean": "rimraf node_modules/.cache/storybook",
    "storybook:start": "start-storybook -p 9003",
    "types": "run-s -cs types:clean types:generate types:fix",
    "types:clean": "rimraf types _types",
    "types:generate": "tsc-silent --project ./tsconfig.json --suppress @ --compiler \"../../node_modules/typescript/lib/typescript.js\"",
    "types:fix": "mv _types types && rimraf _types types/**/mocks"
  },
  "dependencies": {
    "@leemonade/rjsf-core": "^3.0.0",
    "@rjsf/core": "^3.2.1",
    "colord": "^2.9.2",
    "immutability-helper": "^3.1.1",
    "json-rules-engine-simplified": "^0.1.17",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react-hook-form": "^7.22.1",
    "react-router-dom": "^5.3.0",
    "react-table": "^7.7.0",
    "react-table-sticky": "^1.1.3",
    "rjsf-conditionals": "^1.1.4",
    "rrule": "^2.6.8",
    "uuid": "^8.3.2",
    "framer-motion": "6.3.11",
    "ajv": "6.12.6"
  },
  "peerDependencies": {
    "@bubbles-ui/calendars": "^1.2.251",
    "@bubbles-ui/components": "^1.2.251",
    "@bubbles-ui/editors": "^1.2.251",
    "@bubbles-ui/icons": "^1.2.251",
    "dayjs": "^1.10.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "webpack": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-docs": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addons": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/react": "^6.4.9",
    "babel-plugin-dev-expression": "^0.2.3",
    "babel-plugin-transform-import-styles": "^0.0.11",
    "cross-env": "^7.0.3",
    "in-publish": "^2.0.1",
    "load-styles": "^2.0.0",
    "npm-which": "^3.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-import-css": "^3.0.2",
    "rollup-plugin-strip-banner": "^2.0.0",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "storybook-addon-designs": "^6.2.0"
  },
  "resolutions": {
    "@storybook/react/webpack": "^5"
  }
}
